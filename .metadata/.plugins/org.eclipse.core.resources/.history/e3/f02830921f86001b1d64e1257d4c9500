import React, {useState} from 'react'


const BoardList = () => {

    const [ count, setCount ] = useState(0)
    const onIncrease1 = () => {setCount(count + 1)}
    const onDecrease1 = () => {setCount(count - 1)}
    return (<>
    <h1>또 카운터냐 : {count}</h1>
    <button onClick={onIncrease1}>증가</button>
    <button onClick={onDecrease1}>감소</button>
    </>)
}
export default BoardList